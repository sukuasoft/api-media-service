  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.10.1",
    "@types/swagger-ui-express": "^4.1.7",
    "eslint": "^9.16.0",
